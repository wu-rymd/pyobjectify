graft example_project_python

include LICENSE
include README.md

include .bumpversion.cfg
include pyproject.toml
include Makefile

prune .github
exclude .gitignore
exclude .gitattributes

include *.md
include *.yml
recursive-include pyobjectify *.example
recursive-include pyobjectify *.csv
recursive-include pyobjectify *.json
recursive-include pyobjectify *.tsv
recursive-include pyobjectify *.xml
recursive-include pyobjectify *.xlsx
recursive-include pyobjectify *.txt

recursive-include docs *.bat
recursive-include docs *.md
recursive-include docs *.py
recursive-include docs *.rst
recursive-include docs Makefile

recursive-include docs *.buildinfo
recursive-include docs *.css
recursive-include docs *.doctree
recursive-include docs *.eot
recursive-include docs *.html
recursive-include docs *.inv
recursive-include docs *.js
recursive-include docs *.pickle
recursive-include docs *.png
recursive-include docs *.svg
recursive-include docs *.ttf
recursive-include docs *.txt
recursive-include docs *.woff
recursive-include docs *.woff2

recursive-include docs *.nojekyll

# Patterns to exclude from any directory
global-exclude *~
global-exclude *.pyc
global-exclude *.pyo
global-exclude .git
global-exclude .ipynb_checkpoints
global-exclude .DS_Store